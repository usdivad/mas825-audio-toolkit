d on new input\n    var msg;\n    if (triggerPort.name === \"bufIn\") {\n      msg = osc.fromBuffer(ports.bufIn.get());\n      if (msg) {\n        var translated = msg.address.replace(/\\//g,'_');\n        if (ports.hasOwnProperty(translated)) {\n          ports[translated].set(msg.args);\n        }\n      }\n    } else {\n      var ip = triggerPort.get();\n      var values;\n      // Is the intention here to be able to send an array (or object) of arguments? If so, and this works, we don't need OSCArrayMessage.\n      if (ip != null && typeof ip == 'object') {\n        values = [];\n        for (var k in ip) {\n          values.push(ip[k]);\n        }\n      }\n      msg = {\n        address: \"\" + triggerPort.name.replace(/_/g, '/'),\n        args: (values) ? values : ip\n      };\n      ports.bufOut.set(osc.toBuffer(msg));\n      ports.oscOut.set(msg);\n    }\n  }"},"b9d695ac-ea1c-487b-8d06-856fc5c62520":{"nodetype":"ConsoleLogger","descr":"This will log the value which is sent to it","x":640,"y":520,"id":"b9d695ac-ea1c-487b-8d06-856fc5c62520","inputs":{"logInput":{"id":"907e9ad7-f930-4953-9f04-e2590afaf829","value":{"address":"/addr1","args":0.4},"type":"ANY","fixed":false,"published":false,"editor":false,"continuous":true}},"outputs":{"passthruOutput":{"id":"0100c4bd-3465-494f-945e-41d879f81a88","value":{"address":"/addr1","args":0.4},"type":"ANY","editor":false,"fixed":false}},"nodeName":""},"f95afa74-b0bb-4d8e-81b0-19b11e65f88c":{"nodetype":"UDPTransceiver","descr":"Receives and sends UDP datagrams","x":920,"y":280,"id":"f95afa74-b0bb-4d8e-81b0-19b11e65f88c","inputs":{"bufIn":{"id":"ad921952-9a85-4193-abd6-d3f9811dcc4b","value":{"type":"Buffer","data":[47,97,100,100,114,49,0,0,44,102,0,0,62,204,204,205]},"type":"BUF","fixed":false,"published":false,"editor":false,"continuous":true},"oscIn":{"id":"58cf8e5e-2b5f-487a-8028-387a487f7fb9","value":{},"type":"OBJECT","fixed":false,"published":false,"editor":false,"continuous":true},"multiSend":{"id":"a78501d0-0da4-